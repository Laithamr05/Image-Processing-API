{
  "name": "image-processing-api",
  "version": "1.0.0",
  "description": "Node.js + TypeScript image processing API with Sharp, disk caching, tests, linting.",
  "main": "dist/Server/server.js",
  "scripts": {
    "dev": "nodemon --watch . --ext ts --exec ts-node Server/server.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/Server/server.js",
    "test": "jasmine",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check \"**/*.{ts,js,json,md}\"",
    "format:write": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "lsof": "^0.1.0",
    "morgan": "^1.10.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/jasmine": "^5.1.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.18.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "jasmine": "^5.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  }
}
